#: ../../quickstart.rst:30 815fb132cf194d20b2835140ae47ca05
msgid "Basic Usage"
msgstr "Базовое использование"

#: ../../quickstart.rst:32 4c05bb13dc174b59b742f0c68fdc48dc
msgid "Here's a minimal example to get you started:"
msgstr "Вот минимальный пример для начала работы:"

#: ../../quickstart.rst:34 84d2a6e45bdd4b07abba8cf909f26300
msgid ""
"import logging\n"
"from tgbot_logging import TelegramHandler\n"
"\n"
"# Create logger\n"
"logger = logging.getLogger('MyApp')\n"
"logger.setLevel(logging.DEBUG)\n"
"\n"
"# Create TelegramHandler\n"
"telegram_handler = TelegramHandler(\n"
"    token='YOUR_BOT_TOKEN',\n"
"    chat_ids=['YOUR_CHAT_ID'],\n"
"    level=logging.INFO\n"
")\n"
"\n"
"# Add handler to logger\n"
"logger.addHandler(telegram_handler)\n"
"\n"
"# Example usage\n"
"logger.info('This is an info message')\n"
"logger.error('This is an error message')"
msgstr ""

#: ../../quickstart.rst:58 c3c38b6e32714ae88dd2a23f7fa9ffbb
msgid "Using Environment Variables"
msgstr "Использование переменных окружения"

#: ../../quickstart.rst:60 8312dc25630b4127b8b39d5fb3cfdf53
msgid "You can use environment variables for configuration:"
msgstr "Вы можете использовать переменные окружения для конфигурации:"

#: ../../quickstart.rst:62 393686b82a9648749a53df9f69daacb8
msgid ""
"import os\n"
"from dotenv import load_dotenv\n"
"from tgbot_logging import TelegramHandler\n"
"\n"
"# Load environment variables\n"
"load_dotenv()\n"
"\n"
"# Create handler using environment variables\n"
"handler = TelegramHandler(\n"
"    token=os.getenv('TELEGRAM_BOT_TOKEN'),\n"
"    chat_ids=[os.getenv('TELEGRAM_CHAT_ID')],\n"
"    level=os.getenv('LOG_LEVEL', 'INFO'),\n"
"    batch_size=int(os.getenv('BATCH_SIZE', 5)),\n"
"    batch_interval=float(os.getenv('BATCH_INTERVAL', 2.0))\n"
")"
msgstr ""

#: ../../quickstart.rst:81 98acbb224c9f4f6d9d79d40b0722b49a
msgid "Message Formatting"
msgstr "Форматирование сообщений"

#: ../../quickstart.rst:83 cdf6ba6041a8428d81bc191957caa4a6
msgid "HTML Formatting:"
msgstr "HTML форматирование:"

#: ../../quickstart.rst:85 332fdd7720054212830d3649596c9262
#, python-format
msgid ""
"handler = TelegramHandler(\n"
"    token='YOUR_BOT_TOKEN',\n"
"    chat_ids=['YOUR_CHAT_ID'],\n"
"    parse_mode='HTML',\n"
"    fmt='<b>%(levelname)s</b> [%(asctime)s]\\n%(message)s'\n"
")\n"
"\n"
"logger.info('Message with <b>bold text</b> and <i>italic text</i>')"
msgstr ""

#: ../../quickstart.rst:96 4f376e4aff844f398ec19d40f437df7c
msgid "MarkdownV2 Formatting:"
msgstr "MarkdownV2 форматирование:"

#: ../../quickstart.rst:98 8bef4711633040718cb6fc6d665f9089
#, python-format
msgid ""
"handler = TelegramHandler(\n"
"    token='YOUR_BOT_TOKEN',\n"
"    chat_ids=['YOUR_CHAT_ID'],\n"
"    parse_mode='MarkdownV2',\n"
"    fmt='*%(levelname)s* \\[%(asctime)s\\]\\n%(message)s'\n"
")\n"
"\n"
"logger.info('Message with *bold text* and _italic text_')"
msgstr ""

